{
    "navigation" : {
        "welcome" : {
            "link" : "#welcome-to-arm-settings", 
            "content" : "Welcome to ARM-Settings"
        },
        "system_info" : {
            "link" : "#base-infomation", 
            "content" : "Base Infomation"
        },
        "network" : {
            "link" : "#network",
            "content" : "Network"
        },
        "data_and_time" : {
            "link" : "#dateAndTime",
            "content" : "Date & Time"
        }
    },
    "system_info" : {
        "CPU" : {
            "armv71" : "grep Hardware /proc/cpuinfo | awk -F ':' '{print $2}' | sed 's/ Sabre-SD Board//g'",
            "x86_64" : "cat /proc/cpuinfo | grep 'model name' | tail -n 1 | awk -F ':' '{print $2}' | xargs"
        },
        "Memery" : {
            "armv71" : "grep MemTotal /proc/meminfo | awk -F ':' '{print $2}' | awk -F ' ' '{print $1}'",
            "x86_64" : "grep MemTotal /proc/meminfo | awk -F ':' '{print $2}' | awk -F ' ' '{print $1}'"
        },
        "eMMC" : {
            "armv71" : "fdisk -l | grep mmcblk0: | awk -F ':' '{print $2}' | awk -F ',' '{print $1}'",
            "x86_64" : "fdisk -l | grep /dev/sda | awk -F ':' '{print $2}' | awk -F ',' '{print $1}' | head -n 1 | xargs"
        },
        "eMMC_Available" : {
            "armv71" : "df -h | grep /dev/root | awk -F ' ' '{print $4}'",
            "x86_64" : ""
        },
        "Operating_System_Type" : {
            "armv71" : "uname",
            "x86_64" : "uname"
        },
        "Linux_Version" : {
            "armv71" : "uname -a | awk -F ' ' '{print $3}'",
            "x86_64" : "uname -a | awk -F ' ' '{print $3}'"
        }
    },
    "network" : {
        "dhcp" : {
            "check_mode" : {
                "armv71" : "grep 'iface eth0 inet dhcp' /etc/network/interfaces",
                "x86_64" : ""
            }
        },
        "static" : {
            "check_mode" : {
                "armv71" : "grep 'iface eth0 inet static' /etc/network/interfaces",
                "x86_64" : ""
            },
            "ip" : {
                "armv71" : "ifconfig 'eth0' | grep 'inet ' | awk -F ' ' '{print $2}'",
                "x86_64" : ""
            },
            "netmask" : {
                "armv71" : "ifconfig 'eth0' | grep 'inet ' | awk -F ' ' '{print $4}'",
                "x86_64" : ""
            },
            "broadcast" : {
                "armv71" : "ifconfig 'eth0' | grep 'inet ' | awk -F ' ' '{print $6}'",
                "x86_64" : ""
            },
            "gateway" : {
                "armv71" : "route -n | grep UG | head -n  1 | awk -F ' ' '{print $2}'",
                "x86_64" : "route -n | grep UG | head -n  1 | awk -F ' ' '{print $2}'"
            }
        }
    },
    "date_and_time" : {
        "date" : {
            "armv71" : "date '+%Y-%m-%d'",
            "x86_64" : "date '+%Y-%m-%d'"
        },
        "time" : {
            "armv71" : "date '+%H:%M'",
            "x86_64" : "date '+%H:%M'"
        }
    }
}
